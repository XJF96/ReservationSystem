<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhbit.admin.studentclient">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Student"/>
        <activity android:name=".PersonalCenter"/>
        <activity android:name=".Reservation"/>
        <activity android:name=".Succeed"/>
        <activity android:name=".Repair"/>
        <activity android:name=".Opinion"/>
        <activity android:name=".Help"/>
        <activity android:name=".Forum"/>
        <activity android:name=".Register"/>
    </application>
</manifest>